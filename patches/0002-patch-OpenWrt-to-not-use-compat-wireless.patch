--	int highest;
--
--	available = false;
--	bsscfgidx = 2;
--	highest = 2;
--	for (ifidx = 0; ifidx < BRCMF_MAX_IFS; ifidx++) {
--		if (drvr->iflist[ifidx]) {
--			if (drvr->iflist[ifidx]->bsscfgidx == bsscfgidx)
